{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "Bundler",
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES6", "DOM"],

    "jsx": "react",

    "strict": true,
    "skipLibCheck": true,
    "types": ["chrome"]
  }
}
